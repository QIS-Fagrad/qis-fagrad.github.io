import adapter from '@sveltejs/adapter-static';
import preprocess from 'svelte-preprocess';

export default {
	preprocess: preprocess(),
	kit: {
		adapter: adapter({
			pages: 'build',
			assets: 'build',
			fallback: null
		}),
		paths: {
			base: '/my-landing-page' // replace with your repo name
		}
prerender: {
      default: true // prerender all pages it can find
    }
	}
};
